  /*//// Button tab ////*/
  
  "more_options_button@settings_dx.button_tab": {
    "size": [ 150, 20 ],
    "$button_text": "options.keyboardAndMouseSettings",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "$toggle_name": "keyboard_and_mouse_options",
    "$tab_view_binding_name": "keyboard_and_mouse_more_toggle",
    "$toggle_group_forced_index": 1
  },

  "main_options_button@settings_dx.button_tab": {
    "size": [ 200, 20 ],
    "$button_text": "gui.done",
    "$is_back_button": true,
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
